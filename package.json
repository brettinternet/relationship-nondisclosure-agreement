{
  "name": "relationship-nondisclosure-agreement",
  "main": "public/index.html",
  "scripts": {
    "start": "parcel ./src/index.html",
    "prebuild": "npm run clean",
    "build": "parcel build ./src/index.html --out-dir public --public-url ./",
